{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"source": {
		"include": ["src", "api"],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"templates": {
		"better-docs": {
			"name": "My React components"
		}
	},
	"opts": {
		"destination": "docs",
		"recurse": true,
		"readme": "README.md"
	}
}

